{
  "name": "pipelines-ci-cd",
  "version": "1.0.0",
  "description": "Exemplo de pipeline de CI/CD.",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run-chrome": "cypress run --browser chrome",
    "cy:run-firefox": "cypress run --browser firefox",
    "cy:parallel": "cypress run --parallel --record --key $CYPRESS_RECORD_KEY",
    "cy:verify": "cypress verify",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mocha/output.json",
    "generate-report": "marge cypress/reports/mocha/output.json -f report -o cypress/reports/html"
  },
  "keywords": ["ci/cd", "github actions", "jenkins", "pipeline", "azure", "bitbucket", "gitlab", "circleci", "cypress"],
  "author": "Caio Andrian",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^13.6.6",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^2.15.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}